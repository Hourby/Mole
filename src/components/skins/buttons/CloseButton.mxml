<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage smooth="true" source="@Embed('/assets/images/close.png')" x="2" y="1">
		<s:filters>
			<s:GlowFilter includeIn="over" blurX="2.0" blurY="6.0" inner="false" color="#FFFFFF" strength="1" alpha="1.0" quality="2" knockout="false"/>
		</s:filters>
	</s:BitmapImage>

</s:Skin>
